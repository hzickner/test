; enable/disable NMI
PPU_enableNMI:     
__aa6b:     lda PPUSTATUS
            and #$80
            bne __aa6b         ; wait for VBLANK over //TODO safe?     
            lda CTRL
            ora #$80           ; 10000000 set bit 8
            bne __aa7c         ; always jump
PPU_disableNMI:
            lda CTRL
            and #$7f           ; 01111111 unset bit 8    
__aa7c:     sta PPUCTRL
            sta CTRL
            rts

