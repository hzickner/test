            inx
            stx JOY1
            dex
            stx JOY1           ; prepare for reading by writing 1 and 0
            ldx #$08           ; counter
@loop:      lda JOY1           ; read bit
            lsr                ; shift to carry
            rol JOY1_RAW_NEW       ; rol carry into variable
            lsr                ; 
            rol $00            ; rol next bit to TEMP1 variable
            lda JOY2           ; read bit
            lsr
            rol JOY2_RAW_NEW
            lsr
            rol $01
            dex
            bne @loop
            rts

