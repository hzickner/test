            bne @skip
            inc COUNTDOWN      ; reset counter to 0 in case of underflow
@skip:      jsr PPU_updateSprites
            lda FRAMECOUNT
            clc
            adc #$01
            sta FRAMECOUNT
            lda #$00
            adc FRAMECOUNT+1
            sta FRAMECOUNT+1   ; increment frame counter
            ldx #RANDOM
            ldy #$02
            jsr nextRandom     ; new 2 byte random number
            lda #$00
            sta HSCROLL
            sta PPUSCROLL
            sta VSCROLL
            sta PPUSCROLL      ; reset scroll position
            lda #$01
            sta VBLANK         ; set VBLANK flag
            jsr nmi_handle_input
            pla
            tay
            pla
            tax
            pla                ; restore registers
