;-------------------------------------------------------------------------------
; maps tetr id to tetr type
tetr_type_table:
            .hex 00 00 00 00   ; T
            .hex 01 01 01 01   ; J
            .hex 02 02         ; Z
            .hex 03            ; O
            .hex 04 04         ; S
            .hex 05 05 05 05   ; L
            .hex 06 06         ; I
; end of tetr_type_table 19bytes

;-------------------------------------------------------------------------------
; spawn table
; spawn orientation for every tetrimino type            
spawn_table:
            .hex 02 07 08 0a   ; T J Z O
            .hex 0b 0e 12 02   ; S L I T //TODO do we need last entry?
; end of spawn_table 8bytes

;-------------------------------------------------------------------------------
; spawn table
; spawn orientation for every tetrimino orientation
tetr_spawn_table:     
            .hex 02 02 02 02   ; Td
            .hex 07 07 07 07   ; Jd
            .hex 08 08         ; Zh
            .hex 0a            ; O
            .hex 0b 0b         ; Sh
            .hex 0e 0e 0e 0e   ; Ld
            .hex 12 12         ; Ih
; end of spawn_table 19bytes

;-------------------------------------------------------------------------------
; called from game_func1
; increment tetr type counter for statistics (3 digit BCD number in 2bytes)  
; arg A - spawntable entry
tetr_inc_type_counter:
            tax
            lda tetr_type_table,x	; get tetr_type (0..6)
            asl
            tax				; index = 2*type
            lda TYPE_COUNTERS,x        ; load value from ram
            clc
            adc #$01           ; add 1
            sta TEMP2
            and #$0f           ; get lower nibble
            cmp #10
            bmi @skip1         ; if (>=10)
            lda TEMP2
            clc
            adc #$06
            sta TEMP2          ;   value += 6
            cmp #$a0
            bcc @skip1         ;   if (>=$a0)   10 in high nibble
            clc
            adc #$60
            sta TEMP2          ;     value += $60
            lda TYPE_COUNTERS+1,x        ;     read next byte  
            clc
            adc #$01           ;     add 1
            sta TYPE_COUNTERS+1,x        ;     store high byte (3rd digit)
@skip1:     lda TEMP2
            sta TYPE_COUNTERS,x        ; store low byte (1st and 2nd digit)
            lda NMI_FLAGS
            ora #NMI_TYPE_COUNT
            sta NMI_FLAGS            ; set bit6 in NMI_FLAGS
            rts

