; read SPR_PTR_INDEX
; draw this sprite of one or multiple tiles to RAM
spr_drawtoMem:  
            clc
            lda SPR_PTR_INDEX  ; 
            rol                ; //TODO clc+rol -> asl     
            tax                ; X = 2*SPR_PTR_INDEX
            lda sprPTR__table,x
            sta TEMP2
            inx
            lda sprPTR__table,x
            sta TEMP2+1        ; get PTR to sprite to TEMP2     
            ldx OAM_USED       ; get offset to OAMBASE to X
            ldy #$00
@loop:      lda (TEMP2),y      ; while (*TEMP2 != FF) do {
            cmp #$ff
            beq @skip
            clc                ; $8c41: 18        
            adc SPR_Y            ; $8c42: 65 a1     
            sta OAMBASE,x        ; write sprite y
            inx                ; $8c47: e8        
            iny                ; $8c48: c8        
            lda (TEMP2),y      ; $8c49: b1 a8     
            sta OAMBASE,x        ; write sprite index
            inx                ; $8c4e: e8        
            iny                ; $8c4f: c8        
            lda (TEMP2),y      ; $8c50: b1 a8     
            sta OAMBASE,x        ; write sprite attributes
            inx                ; $8c55: e8        
            iny                ; $8c56: c8        
            lda (TEMP2),y        ; $8c57: b1 a8     
            clc                ; $8c59: 18        
            adc SPR_X            ; $8c5a: 65 a0     
            sta OAMBASE,x        ; write sprite x
            inx                ; $8c5f: e8        
            iny                ; $8c60: c8        
            lda #$04           ; $8c61: a9 04     
            clc                ; $8c63: 18        
            adc OAM_USED            ; //TODO write x ->OAM_USED
            sta OAM_USED            ; $8c66: 85 b3     
            jmp @loop          ; } end while
@skip:      rts

