            sta SPR_PTR_INDEX  ; } else SPR_PTR_INDEX = 83     
@skip8:     jsr spr_drawtoMem
            jsr frame_clear_sprite_ram
            jmp loop__830b

;-------------------------------------------------------------------------------
; level selection
mode_func_level:
            inc $8000          ; reset MMC1
            lda #$10           ; mirrorring: one screen; PRG: 32K $8000 switchable at $C000; CHR: switch 2 4K banks
            jsr MMCsetreg0
            jsr snd_reset_call
            lda #$01
            sta NMI_FUNC_INDEX ; set nmi function for this mode (write PPUCTRL with nametable 0, write scroll regs with 0)
            jsr frame_PPU_rendering_off
            jsr PPU_disableNMI
            lda #$00
            jsr MMCsetreg1
            lda #$00
            jsr MMCsetreg2     ; CHR bank 0
  
            jsr PPU_copy_data
            DW PALETTE_DATA_MODE1_2_3

            jsr PPU_copy_data
            DW LEVEL_SCREEN_DATA

            lda SEL_TYPE
            bne @skip1
                               ; if (type A) user level_screen_data2
            jsr PPU_copy_data
            DW LEVEL_SCREEN_DATA2

@skip1:     jsr PPU_writeHSdata
            jsr PPU_enableNMI
            jsr frame_clear_sprite_ram
            lda #$00
            sta PPUSCROLL
            lda #$00
            sta PPUSCROLL
            jsr frame_PPU_rendering_on
            jsr frame_clear_sprite_ram
            lda #$00
            sta B1
            sta B5
@loop1:     lda P1_LEVEL
            cmp #10
            bcc @skip2         ; if (A < 10) skip2
            sec
            sbc #$0a
            sta P1_LEVEL
            jmp @loop1         ; LEVEL = LEVEL mod 10

@skip2:
@loop0:     lda #$00
            sta CUR_PLR            ; CUR_PLR = 0 //TODO remove: not used
            lda P1_LEVEL
            sta SEL_LEVEL
            lda P1_HEIGHT
            sta SEL_HEIGHT
            lda B1
            sta B4           ; init level height and selection flag
            lda JOY1_RAW_NEW
            sta BUTTONS_NEW
            jsr update_sel_sprites
            lda SEL_LEVEL
            sta P1_LEVEL
            lda SEL_HEIGHT
            sta P1_HEIGHT
            lda B4
            sta B1             ; save selected values
            lda JOY1_RAW_NEW
            cmp #BUTTON_START
            bne @not_start     ; if (BUTTON_Start) {
            lda JOY1_RAW_ALL
            cmp #BUTTON_A | BUTTON_START
            bne @skip3         ;   if (JOY1_RAW_ALL == 144) {
            lda P1_LEVEL
            clc
            adc #10            ;     LEVEL += 10
            sta P1_LEVEL            ;   } endif
@skip3:     lda #$00
            sta GAME_FUNC_INDEX            ; GAME_FUNC_INDEX = 0
            lda #$02
            sta sndEFFECT
            inc GAME_MODE       
            rts                ; exit to next mode_game } endif 

@not_start: lda JOY1_RAW_NEW
            cmp #BUTTON_B
            bne @not_B         ; if (BUTTON_B) {
            lda #$02
            sta sndEFFECT
            dec GAME_MODE
            rts                ; exit to previous mode_type } endif

@not_B:
@loop2:     ldx #RANDOM
            ldy #$02
            jsr nextRandom
            lda RANDOM
            and #$0f
            cmp #$0a
            bpl @loop2         ; get RANDOM < 10 (0..9)
            sta P1_SPACE_COLUMN            ; P1_SPACE_COLUMN = rnd(0..9)
@loop3:     ldx #RANDOM
            ldy #$02
            jsr nextRandom
            lda RANDOM
            and #$0f
            cmp #$0a
            bpl @loop3         ; get RANDOM < 10 (0..9)
            sta P2_SPACE_COLUMN            ; P2_SPACE_COLUMN = rnd(0..9)
            jsr frame_clear_sprite_ram
            jmp @loop0         ; next frame


;-------------------------------------------------------------------------------
; updates sprites for level and lines selection
; B4- flag: 0 level select, 1 height sel
update_sel_sprites:
            lda BUTTONS_NEW
            cmp #BUTTON_R
            bne @skip2         ; if (BUTTON_R) {
            lda #$01
            sta sndEFFECT
            lda B4
            bne @skip1
            lda SEL_LEVEL
            cmp #$09
            beq @skip2
            inc SEL_LEVEL
            jmp @skip2
@skip1:     lda SEL_HEIGHT
            cmp #$05
            beq @skip2
            inc SEL_HEIGHT     ;   increment level or height dependent on B4up to maxvalue
                               ; } endif      
@skip2:     lda BUTTONS_NEW
            cmp #BUTTON_L
            bne @skip4         ; if (BUTTON_L) {
            lda #$01
            sta sndEFFECT
            lda B4
            bne @skip3
            lda SEL_LEVEL
            beq @skip4
            dec SEL_LEVEL
            jmp @skip4
@skip3:     lda SEL_HEIGHT
            beq @skip4
            dec SEL_HEIGHT     ;   decrement level or height dependent on B4minvalue=0
                               ; } endif
@skip4:     lda BUTTONS_NEW
            cmp #BUTTON_D
            bne @skip6         ; if (BUTTON_D) {
            lda #$01
            sta sndEFFECT
            lda B4
            bne @skip5
            lda SEL_LEVEL
            cmp #$05
            bpl @skip6
            clc
            adc #$05
            sta SEL_LEVEL
            jmp @skip6
@skip5:     lda SEL_HEIGHT
            cmp #$03
            bpl @skip6
            inc SEL_HEIGHT
            inc SEL_HEIGHT
            inc SEL_HEIGHT     ;   next level or height row dependent on B4//TODO clc adc sta vs 3x inc
                               ; } endif
@skip6:     lda BUTTONS_NEW
            cmp #BUTTON_U
            bne @skip8         ; if (BUTTON_U) {
            lda #$01
            sta sndEFFECT
            lda B4
            bne @skip7
            lda SEL_LEVEL
            cmp #$05
            bmi @skip8
            sec
            sbc #$05
            sta SEL_LEVEL
            jmp @skip8
@skip7:     lda SEL_HEIGHT
            cmp #$03
            bmi @skip8
            dec SEL_HEIGHT
            dec SEL_HEIGHT
            dec SEL_HEIGHT     ;   prev level or height row dependent on B4//TODO sec sbc sta vs 3x dec
                               ; } endif
@skip8:     lda SEL_TYPE
            beq @level_spr
            lda BUTTONS_NEW
            cmp #BUTTON_A
            bne @level_spr     ; if (BUTTON_A && typeB) {
            lda #$01
            sta sndEFFECT
            lda B4
            eor #$01
            sta B4           ;   toggle B4 flag
                               ; } endif     
@level_spr: lda B4            ; B4 toggles flickering, only active selector flickers
            bne @skip10
            lda FRAMECOUNT     ; if (B4 == 0) {
            and #$03
            beq @lines_spr:       ;   dont draw level_sel_sprite every 4th frame
@skip10:    ldx SEL_LEVEL
            lda SPR_SEL_Y_TABLE,x  ; get y coord from table
            sta SPR_Y
            lda #$00
            sta SPR_PTR_INDEX   ; use sprite 0
            ldx SEL_LEVEL       ; //TODO: remove reload of X
            lda SPR_SEL_X_TABLE,x   ; get x coord from table
            sta SPR_X
            lda CUR_PLR
            cmp #$01
            bne @skip11:       ; if (CUR_PLR == 1) { //TODO remove: never true
            clc
            lda SPR_Y
            adc #80            ;   SPR_Y += 80
            sta SPR_Y          ; } endif
@skip11:    jsr spr_drawtoMem  ; draw level_sel_sprite

@lines_spr: lda SEL_TYPE
            beq @ret           ; if (typeA) return
            lda B4
            beq @skip13:       ; if (!active line_sel) skip blinking
            lda FRAMECOUNT
            and #$03
            beq @ret
@skip13::   ldx SEL_HEIGHT
            lda SPR_LINE_SEL_Y_TABLE,x
            sta SPR_Y          ; y coord
            lda #$00
            sta SPR_PTR_INDEX  ; use sprite0
            ldx SEL_HEIGHT
            lda SPR_LINE_SEL_X_TABLE,x
            sta SPR_X          ; x coord
            lda CUR_PLR
            cmp #$01
            bne @skip14:       ; if (CUR_PLR == 1) { //TODO remove: never true
            clc
            lda SPR_Y
            adc #$50           ;   SPR_Y += 80
            sta SPR_Y          ; } endif
@skip14::   jsr spr_drawtoMem
@ret:     rts

