PPU_copy_data:     
            jsr get_pointer    ; read ROM stored pointer to TEMP1
            jmp process_data         ; $aa9b: 4c f2 aa  

;-------------------------------------------------------------------------------
copy_PPU_bytes:     
            pha                ; save high byte PPU address read from TEMP1 pointer
            sta PPUADDR
            iny
            lda (TEMP1),y      ; read next byte from TEMP1 pointer
            sta PPUADDR        ; set PPU address
            iny
            lda (TEMP1),y      ; read next byte from TEMP1 pointer (bit7 VRAM inc line, bit6 ptr inc off, bit 0..5 byte counter)
            asl                ; A *= 2, bit7 to carry, toggles VRAM address increment
            pha                ; save A
            lda CTRL
            ora #$04           ; VRAM address increment: down
            bcs @skip
            and #$fb           ; VRAM address increment: right
@skip:      sta PPUCTRL        ; set VRAM address increment
            sta CTRL
            pla                ; restore A
            asl                ; A *= 2, original bit 6 to carry
            php                ; save flags

            bcc @skip2         ; if (bit6 of control byte) {
            ora #$02           ;   A: set bit 2, controls increment of TEMP1 ptr
            iny                ;   shift read index from control to data byte (not incremented in read loop)
                               ; } endif

@skip2:     plp                ; restore flags (flags after 2 asl of original A)
            clc
            bne @skip3
            sec                ; if lower 6bits of original A clear set carry else clear (0 means write 128bytes)
@skip3:     ror                ; rotate carry to bit 7
            lsr                ; carry is in bit 6 bits 0..5 are equal to original A, carry is ptr increment flag
            tax                ; initialize counter
@loop:      bcs @skip4
            iny                ; if !carry increment pointer
@skip4:     lda (TEMP1),y
            sta PPUDATA
            dex
            bne @loop          ; read X bytes from TEMP1 ptr carry controls if ptr is incremented, write to PPUDATA
            pla                ; restore high byte of PPU address
            cmp #$3f
            bne next_address         ; if pallette address {
            sta PPUADDR
            stx PPUADDR        ; set PPU address to $3F00
            stx PPUADDR
            stx PPUADDR        ; set PPU address to $0000 //TODO 4 writes to PPUADDR?
                               ; } endif

next_address:
            sec
            tya
            adc TEMP1
            sta TEMP1
            lda #$00
            adc TEMP1+1
            sta TEMP1+1         ; add number of bytes read to TEMP1 ptr
process_data:
@loop:      ldx PPUSTATUS      ; reset PPU address latch
            ldy #$00           ; Y=0
            lda (TEMP1),y
            bpl @skip
            rts                ; while *TEMP1 >= 0

@skip:      cmp #$60
            bne @skip2         ; if (*TEMP1 == $60)
            pla
            sta TEMP1+1
            pla
            sta TEMP1
            ldy #$02           ; TEMP1 = pop; Y=2
            bne next_address   ; always jump

@skip2:     cmp #$4c
            bne copy_PPU_bytes ; if (*TEMP1 == $4c)
            lda TEMP1
            pha
            lda TEMP1+1
            pha                ; push TEMP1 - manipulates return address
            iny                ; Y = 1
            lda (TEMP1),y
            tax                ; X = *(TEMP1+1)
            iny
            lda (TEMP1),y      ; A = *(TEMP+2)
            sta $01
            stx $00            ; TEMP = AX
            bcs @loop          ; end while

;-------------------------------------------------------------------------------
; called from PPU_copy_data
; read pointer which is stored at return address of PPU_copy_data caller to TEMP1
; set return address for PPU_copy_data caller after ROM stored pointer 
get_pointer:
            tsx                ; get stack pointer
            lda $0103,x
            sta $05
            lda $0104,x
            sta $06            ; get return address of original caller
            ldy #$01
            lda ($05),y        ; $ab2e: b1 05     
            sta TEMP1
            iny                ; $ab32: c8        
            lda ($05),y        ; $ab33: b1 05     
            sta TEMP1+1        ; get data pointer from next byte
            clc                ; $ab37: 18        
            lda #$02           ; $ab38: a9 02     
            adc $05            ; $ab3a: 65 05     
            sta $0103,x        ; $ab3c: 9d 03 01  
            lda #$00           ; $ab3f: a9 00     
            adc $06            ; $ab41: 65 06     
            sta $0104,x        ; set return address of original caller after data pointer
            rts

