; write packed BCD
; writes 2 decimal digits coded in high and low 4 bits of A to screen
PPUwrite_packedBCD:     sta TEMP2
            and #$f0
            lsr
            lsr
            lsr
            lsr
            sta PPUDATA
            lda TEMP2
            and #$0f
            sta PPUDATA
            rts

