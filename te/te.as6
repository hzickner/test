; check if P1_Score is a new hihgscore
game_check_highscore:
	lda #$00
	sta $d5
	lda SEL_TYPE
	beq @skip1
	lda #$04
	sta $d5			; typeA d5 = 0, typeB d5=4
@skip1:
@loop1:	lda $d5
	sta TEMP2+1
	asl
	clc
	adc TEMP2+1		; *3 -> index for 3 byte entry table
	tay
	lda HS_POINTS,y
	cmp P1_SCORE+2		; compare 1st 2 digits
	beq @skip2		; = compare next digits
	bcs @skip4		; < check next entry
	bcc __a134		; > write new entry
@skip2:	iny
	lda HS_POINTS,y
	cmp P1_SCORE+1
	beq @skip3
	bcs @skip4
	bcc __a134
@skip3:	iny
	lda HS_POINTS,y
	cmp P1_SCORE		; compare last 2 digits
	beq __a134		; = write new entry
	bcc __a134		; > write new entry
@skip4:	inc $d5
	lda $d5			; next high score entry
	cmp #$03
	beq @ret
	cmp #$07
	beq @ret
	jmp @loop1

@ret:	rts

