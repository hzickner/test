;-------------------------------------------------------------------------------
; fill PPU nametable (960bytes) and attribute table (64bytes)
; X=#$FF Y=#$00 arg A
; A - high byte of PPU address ( $20 $24 $28 $2c are valid )
; X - fill value for attribute table
; Y - fill value for attribute table
PPU_fill_name_attribute:
            sta $00
            stx $01
            sty $02            ; save args
            lda PPUSTATUS      ; reset PPU address latch
            lda CTRL
            and #%11111011     ; 11111011 unset bit 2 of CTRL (VRAM writes line)
            sta PPUCTRL
            sta CTRL
            lda $00
            sta PPUADDR
            ldy #$00
            sty PPUADDR        ; set PPU address to $(AA)00
            ldx #$04
            cmp #$20	
            bcs @skip1         ; check if PPU address >= $2000 //TODO always >=$2000 eliminate branch
            ldx $02            ; if not write argY pages else write 4 pages
@skip1:     ldy #$00
            lda $01            ; set write value to argX
@loop1:     sta PPUDATA
            dey
            bne @loop1
            dex
            bne @loop1         ; fill X pages in PPU with #$FF
            ldy $02            ; Y=0 new fill value
            lda $00            ; A=argA
            cmp #$20           ;
            bcc __ac67
            adc #$02           ; if A >= 20 { A+=3 (carry is set) //TODO always >=$2000 eliminate branch
            sta PPUADDR
            lda #$c0
            sta PPUADDR        ; set PPU address to $(argA+3)C0
            ldx #$40
@loop2:     sty PPUDATA
            dex
            bne @loop2         ; clear last 64bytes of PPUDATA page
__ac67:     ldx TEMP1+1         ; } //TODO return value needed?
            rts

;-------------------------------------------------------------------------------
; fill mem pages with A
; args A, X, Y
; A - value to write
; X - first page
; Y - last page
; //TODO optimize: nr of pages as 2nd argument
memset:     pha                ; save arg A
            txa                ; A=X
            sty TEMP1+1         ; set high byte of target
            clc
            sbc TEMP1+1         ; A=X-Y-1
            tax                ; X = -number of pages
            pla                ; restore arg A
            ldy #$00
            sty TEMP1           ; set low byte of target
@loop:      sta (TEMP1),y
            dey
            bne @loop          ; page loop
            dec TEMP1+1            ; dec high byte of target address
            inx
            bne @loop          ; do more pages  
            rts

;-------------------------------------------------------------------------------
; jump table
; arg A index to jump table
; base of jump table (top of stack)+1
; dynamic jump
table_jump: asl                ; index to jump table = 2*A (2 byte entries)
            tay
            iny                ; increment because return address = jumptable-1
            pla
            sta TEMP1
            pla
            sta TEMP1+1         ; PTR=return address
            lda (TEMP1),y
            tax
            iny
            lda (TEMP1),y       ; read 2 bytes of jump address
            sta TEMP1+1
            stx TEMP1           ; store to PTR
            jmp (TEMP1)         ; jmp PTR

