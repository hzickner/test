            .hex ff ff ff 3c   ; $86cc: ff ff ff 3c   Data
            .hex fe
            .hex 23 57
            .hex 3d   ; $86d0: fe 23 57 3d   Data
            .hex 3e 3e 3e 3e   ; $86d4: 3e 3e 3e 3e   Data
            .hex 3e 3e 3f
