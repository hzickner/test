; next random at zp address X of size Y
; //TODO hardcode X,Y remove ld statements before call
nextRandom: lda $00,x
            and #$02
            sta $00
            lda $01,x
            and #$02
            eor $0             ; xor bit 1 of X with bit 1 of X+1
            clc
            beq __ab57
            sec                ; xor result is in carry bit
__ab57:     ror $00,x
            inx
            dey
            bne __ab57         ; rotate with carry Y bytes
            rts

