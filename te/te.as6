frame_clear_sprite_ram:     
            jsr snd__e000         ; $aa2f: 20 00 e0  
            lda #$00
            sta VBLANK
            nop
@loop :     lda VBLANK
            beq @loop          ; wait for vblank
            lda #$ff
            ldx #>OAMBASE
            ldy #>OAMBASE
            jsr memset         ; set sprite data to $ff
            rts

;-------------------------------------------------------------------------------
; waits for vblank
frame_func: 
            jsr snd__e000      ; $aa45: 20 00 e0  
            lda #$00
            sta VBLANK
            nop
@loop:      lda VBLANK
            beq @loop          ; wait for VBLANK
            rts

;-------------------------------------------------------------------------------
; waits for vblank
; disable rendering of sprites and background
frame_PPU_rendering_off:
            jsr frame_func
            lda vPPUMASK
            and #$e1           ; 11100001 bits unsetset in PPUMASK (show sprites and background)
__aa59:     sta PPUMASK
            sta vPPUMASK
            rts

;-------------------------------------------------------------------------------
; waits for vblank
; enable rendering of sprites and background
frame_PPU_rendering_on:
            jsr frame_func
            jsr PPU_resetSCROLL
            lda vPPUMASK
            ora #$1e           ; 11110 bits set in PPUMASK (show sprites and background)
            bne __aa59         ; always jump

