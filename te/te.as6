; main
;-------------------------------------------------------------------------------
main:
reset:      cld                ;
            sei                ; set cpu mode       
            ldx #$00           ;
            stx PPUCTRL        ;
            stx PPUMASK        ; init PPU
__ff0a:     lda PPUSTATUS      ;
            bpl __ff0a         ;
__ff0f:     lda PPUSTATUS      ;
            bpl __ff0f         ; wait for 2 vblanks (bit 7 of PPUSTATUS set)
            dex                ; x = #$FF
            txs                ; stack init
            inc reset          ; reset mmc1 register write with bit7 set to ROM
            lda #$10           ; mirroring: one-screen, lower bank; PRG ROM: switch 32 KB at $8000; CHR ROM bank mode: 2 separate 4K banks
            jsr MMCsetreg0     ;   
            lda #$00           ; CHR bank at PPU $0000      
            jsr MMCsetreg1     ;   
            lda #$00           ; CHR bank at PPU $1000     
            jsr MMCsetreg2     ;  
            lda #$00           ; PRG ROM bank     
            jsr MMCsetreg3     ; init mmc1
            jmp main2          ; 

